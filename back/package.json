{
  "name": "back",
  "version": "1.0.0",
  "description": "Express TypeScript backend server",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "check": "tsc --noEmit && eslint .",
    "typecheck": "npm run check",
    "lint": "npm run check",
    "lint:fix": "eslint . --fix",
    "build": "tsc"
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.2",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.51.0"
  }
}
