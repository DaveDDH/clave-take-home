{
  "name": "back",
  "version": "1.0.0",
  "description": "Express TypeScript backend server",
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "check": "tsc --noEmit && eslint .",
    "typecheck": "npm run check",
    "lint": "npm run check",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "test:queries": "node test-queries.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage"
  },
  "dependencies": {
    "@ai-sdk/groq": "^3.0.4",
    "@ai-sdk/openai": "^3.0.7",
    "@ai-sdk/xai": "^3.0.3",
    "@helicone/ai-sdk-provider": "^1.0.12",
    "ai": "^6.0.23",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@flydotio/dockerfile": "^0.7.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.27",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.51.0"
  }
}
